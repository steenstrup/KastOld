@model IEnumerable<Kast.Models.Topic>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/Content/Style.css" rel="stylesheet">
    <title>KaSt</title>
</head>
<body>
    <div class="grid">
        @foreach (var topic in Model)
        {
            <a href=http://localhost:53087/@topic.pageRedirector.GetUrl()>
                <div class="blocks">
                    <img src=@topic.ThumnailImageUrl>
                    <div class="blockText">@topic.Name</div>
                </div>
            </a>
        }
    </div>
</body>
</html>